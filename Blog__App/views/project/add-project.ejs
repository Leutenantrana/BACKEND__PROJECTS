<a href="/projects">&larr; Back</a>
<div class="admin-title">
    <h2>Add new Post</h2>

</div>


<form action="/projects" method="POST">
    <label for="title"><b>Title</b></label>
    <input type="text" placeholder="New Project Title" name="title">

    <label for="body"><b>Body</b></label>
    <textarea name="body" cols="50" rows="50"></textarea>

    <label for="imagePath"><b>ImagePath</b></label>
    <input type="text" placeholder="/img/" name="imagePath">
    <label for="gitURL"><b>Github-address</b></label>
    <input type="url" placeholder="project git url" name="gitURL">
    <br>
    <br>
    <br>

    <input type="submit" class="add" class="btn">




</form>