<a href="/projects">&larr; Back</a>
<div class="admin-title">
    <h2>View / Edit Post</h2>

    <form action="/delete-project/<%= data._id %>?_method=DELETE" method="POST">
        <input type="submit" value="Delete" class="btn-delete btn">
    </form>

</div>

<form action="/edit-project/<%= data._id %>?_method=PUT" method="POST">

    <label for="title"><b>Title</b></label>
    <input type="text" placeholder="Post Title" name="title" value="<%= data.title %>">

    <label for="body"><b>Content</b></label>
    <textarea name="body" cols="50" rows="10"><%= data.body %></textarea>

    <label for="imagePath"><b>ImagePath</b></label>
    <input type="text" name="imagePath" value="<%= data.imageURL %>">
    <label for="gitURL"><b>Github-address</b></label>
    <input type="url" placeholder="project git url" name="gitURL" value="<%= data.githubAddress %>">


    <input type="submit" value="Update" class="btn">







</form>