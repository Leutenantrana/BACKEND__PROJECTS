<div class="projectHeader">
    <h1>Projects</h1>
    <a href="/add-project" class="button">+ Add New Project</a>

</div>

<ul class="admin-post">
    <% data.forEach(project =>{ %>
        <li class="admin-lis">
            <a href="/project/<%= project._id  %>">
                <%= project.title %>&nearr;

            </a>
            <div class="admin-post-controls">
                <a href="/edit-project/<%= project._id %>" class="btn">Edit</a>
                <form action="/delete-project/<%= project._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="DELETE" class="btn">
                </form>
            </div>
        </li>

        <% })  %>
</ul>